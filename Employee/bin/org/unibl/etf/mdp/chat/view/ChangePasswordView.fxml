<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="ap" fx:controller="org.unibl.etf.mdp.chat.view.ChangePasswordViewController" minHeight="300.0" minWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
	   <Label layoutX="30.0" layoutY="50.0" text="Stara lozinka" />
	   <TextField fx:id="txtOldPassword" layoutX="140.0" layoutY="50.0" />
	   <Label layoutX="30.0" layoutY="100.0" text="Nova lozinka" />
	   <PasswordField fx:id="txtNewPassword" layoutX="140.0" layoutY="100.0" />
	   <Label layoutX="30.0" layoutY="150.0" text="Nova lozinka" />
	   <PasswordField fx:id="txtNewPasswordAgain" layoutX="140.0" layoutY="150.0" />
	   <Button fx:id="btnChangePassword" layoutX="80.0" layoutY="200.0" mnemonicParsing="false" onAction="#changePassword" text="OK" />
	   <Button fx:id="btnCancel" layoutX="150.0" layoutY="200.0" mnemonicParsing="false" onAction="#cancel" text="Nazad" />
   </children>
</AnchorPane>
